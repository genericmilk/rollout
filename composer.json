{
    "name": "genericmilk/rollout",
    "description": "Time delay your laravel controller and blade code into a simple if branch!",
    "keywords": ["laravel", "api", "curl", "get", "post"],
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Day",
            "email": "peterday.main@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "autoload": {
        "psr-4": {
            "Genericmilk\\Rollout\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "3.2-dev"
        },
        "laravel": {
            "providers": [
                "Genericmilk\\Rollout\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/framework": "5.5.x"
    }
}
